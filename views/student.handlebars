<div class="teacher-page" id="teacher">

    <body class="teacher-landing-page">
        <div class="login-card">
            <div class="login-card-header">
                <h2>Welcome Student!</h2>
                <h3>Click your Teacher's name</h3>
                <ul class="nav">
                    {{#each teachers as |teacher|}}
                    <li class="nav-item">
                        <button class="roster-btn">
                            <a class="nav-link active" href="/dashboard/student/{{teacher.id}}" id="{{teacher.id}}">{{teacher.first_name}} {{teacher.last_name}}</a>
                        </button>
                    </li>
                    {{/each}}
                    <li class="nav-item">
                        <button class="back-btn" id="teacher-btn">
                            <a class="nav-link active" href="/dashboard">BACK</a>
                        </button>
                    </li>
                    <ul id="roster-el">

                    </ul>
                </ul>
            </div>
        </div>
</div>