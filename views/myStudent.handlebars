<div class="teacher-page" id="teacher">

    <body class="teacher-landing-page">
        <div class="login-card">
            <div class="login-card-header">
                <h2>{{teacher.first_name}} {{teacher.last_name}}'s Students</h2>
                
                <ul class="nav">
                    {{#each students as |student|}}
                    <li class="nav-item">
                        <button class="roster-btn">
                            <a class="nav-link active" href="/dashboard/student/{{student.id}}" id="{{student.id}}">{{student.first_name}} {{student.last_name}}</a>
                        </button>
                    </li>
                    {{/each}}
                    <li class="nav-item">
                        <button class="back-btn">
                            <a class="nav-link active" href="/dashboard">BACK</a>
                        </button>
                    </li>
                    <ul id="roster-el">

                    </ul>
                </ul>
            </div>
        </div>
</div>